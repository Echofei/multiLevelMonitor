{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
     "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["react","jsx-a11y","redux-saga"],
    "rules": {
         "simi":0,
         "no-new":0,
         "guard-for-in":0,
         "global-require":0,
         "no-restricted-syntax":0,
         "consistent-return":0,
         "no-plusplus":0,
         "no-path-concat":0,
         "prefer-arrow-callback":0,
         "prefer-template":0,
         "quote-props":0,
         "eol-last":0,
         "no-cond-assign": [2, "always"], //if, while等条件中不允许使用“=”
         "no-constant-condition": 2,
         "no-debugger": 2, // 程序中不能出现debugger
         "no-dupe-args": 2, // 函数的参数名称不能重复
         "no-dupe-keys": 2, // 对象的属性名称不能重复
         "no-duplicate-case": 2, // switch的case不能重复
         "no-func-assign": 2,
         "no-obj-calls": 2,
         "key-spacing":0,
         "camelcase":2,
         "quotes":0,
         "object-curly-spacing":0,
         "no-underscore-dangle":0,
         "no-console":0,
         "comma-spacing":0,
         "no-unused-vars":2,//定义变量未使用
         "no-regex-spaces": 2,
         "no-sparse-arrays": 2,
         "no-unexpected-multiline": 2,
         "no-unreachable": 2,
         "use-isnan": 2,
         "valid-typeof": 2,
         "eqeqeq": [2, "always"],
         "no-caller": 2,
         "indent": 2,
         "spaced-comment":0,
         "no-eval": 2,
         "no-redeclare": 2,
         "no-undef": 2,
         "constructor-super":2,
         "no-var":2,
         "no-duplicate-imports":2,
         "prefer-const":2,
         "no-use-before-define": 2,
         "comma-dangle": [1, "never"],
         "no-const-assign": 2,
         "no-dupe-class-members": 2,
         "object-shorthand": 0,
         "no-unused-vars":[2, { "args": "none" }],
         "import/imports-first": 0,
         "import/first":0,
         "import/newline-after-import": 0,
         "import/no-dynamic-require": 0,
         "import/no-extraneous-dependencies": 0,
         "import/no-named-as-default": 0,
         "import/no-unresolved": 0,
         "import/prefer-default-export": 0,
         "import/extensions": 0,
         "react/forbid-prop-types": 0,
         "react/jsx-first-prop-new-line": [
             2,
             "multiline"
           ],
         "react/jsx-filename-extension": 0,
         "react/jsx-closing-bracket-location":0,
         "react/jsx-no-target-blank": 0,
         "react/require-extension": 0,
         "react/self-closing-comp": 0,
         "react/jsx-indent": 0,
         "react/jsx-indent-props": [
             0,
             "tab"
           ],
         "react/prefer-stateless-function": 0,
         "react/prop-types":0,
         "react/jsx-tag-spacing":0,
         "redux-saga/no-yield-in-race": 2,
         "redux-saga/yield-effects": 2,
         "require-yield": 0,
         "jsx-a11y/aria-props": 2,
         "jsx-a11y/heading-has-content": 0,
         "jsx-a11y/href-no-hash": 0,
         "jsx-quotes":0,
         "jsx-a11y/label-has-for": 2,
         "jsx-a11y/mouse-events-have-key-events": 2,
         "jsx-a11y/role-has-required-aria-props": 2,
         "jsx-a11y/role-supports-aria-props": 2
    }
}